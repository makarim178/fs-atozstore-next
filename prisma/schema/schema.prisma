generator client {
  provider = "prisma-client-js"
  output   = "../../src/generated/prisma"
}

generator zod {
  provider = "prisma-zod-generator"
  output   = "../../src/generated/zod"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}